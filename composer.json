{
  "name": "leoruhland/cakeadmin-pluginmanager",
  "description": "PluginManager plugin for CakePHP",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.4",
    "cakephp/plugin-installer": "*",
    "cakephp/cakephp": "~3.0",
    "composer/composer": "*"
  },
  "require-dev": {
    "phpunit/phpunit": " 4.6.*@dev"
  },
  "extra": {
    "installer-name": "PluginManager"
  },
  "autoload": {
    "psr-4": {
      "PluginManager\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PluginManager\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
      "App\\": "tests/App"
    }
  }
}
